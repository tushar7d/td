{
  "name": "tailwindcss-parcel-starter",
  "version": "1.0.0",
  "description": "A simple tailwind starter project with parcel",

  "source": "src/index.html",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "targets": {
    "frontend": {
      "source": "app/index.html"
    }
  },

  "scripts": {
    "dev": "npx parcel src/index.html",
    "build": "parcel build src/index.html"
  },
  
  "devDependencies": {
    "autoprefixer": "^10.4.0",
    "parcel": "2.0.1",
    "postcss": "^8.4.3",
    "postcss-cli": "9.0.2",
    "tailwindcss": "^2.2.19",
    "typescript": "4.6.4"
  },
  "dependencies": {
    "locomotive-scroll": "^4.1.3",
    "swiper": "^7.3.1"
  }
}
